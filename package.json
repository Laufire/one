{
  "name": "@laufire/one",
  "version": "0.0.1",
  "description": "A simple node library to help with declarative programming.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest --no-cache",
    "lint": "eslint ./",
    "build": "babel -d ./dist ./src",
    "link": "npm build && npm link"
  },
  "engines": {
    "node": ">=7.6"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Laufire/one"
  },
  "keywords": [],
  "author": "Laufire Technologies",
  "license": "ISC",
  "homepage": "https://github.com/Laufire/one",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-cli": "^6.26.0",
    "acorn": "^6.0.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "regenerator-runtime": "^0.13.2"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "./.jest-coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "@laufire/utils": "0.0.29"
  }
}
